{% comment %}
  {%- render 'product-grid-item', product: product -%}
{% endcomment %}

<div data-api-content>
  {% unless product.tags contains 'hidden' %}
  {%- render 'product-grid-item', product: product, aos_delay: '||itemAnimationDelay||', aos_anchor: '||itemAnimationAnchor||' -%}
  {% endunless %}
</div>